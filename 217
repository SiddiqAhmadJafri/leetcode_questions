#include <vector>
#include <unordered_set>

class Solution {
public:
    bool containsDuplicate(std::vector<int>& nums) {
        std::unordered_set<int> seen_numbers;
        for (int num : nums) {
            if (seen_numbers.count(num)) { // Check if the number is already in the set
                return true; // Duplicate found
            }
            seen_numbers.insert(num); // Add the number to the set
        }
        return false; // No duplicates found
    }
};
